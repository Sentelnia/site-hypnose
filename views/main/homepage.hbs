<h2 class="title-article">ARTICLE DU MOMENT</h2>
<section id="article-du-moment">
    <div class="arr-left"> <img src="images/arrow-left-slider.png" alt=""></div>
    <div class="arr-right"> <img src="images/arrow-right-slider.png" alt=""> </div>

    <div class="container-slider">


        {{#each articles}}
        <div class="container-article-hp">

            <img src="{{this.imageUrl}}" alt="">

            <div class="container-infos">
                <h3>{{this.title}}</h3>
                <span id="article-date">{{this.createdAt2}}</span>
                <p>{{this.description}}</p>
                <a href="/articles/{{title2}}/{{id}}">EN SAVOIR PLUS <img class="arrow-link"
                        src="images/arrow-right.png" alt="arrow-link"></a>
            </div>
        </div>
        {{/each}}


    </div>

</section>

<div class="link-articles"><a href="/articles">TOUS LES ARTICLES <img class="arrow-link" src="images/arrow-right.png"
            alt="arrow-link"></a></div>




<h2 class="title-article">VOS TEMOIGNAGES</h2>
<section id="temoignages">


    <div id="slider">
        {{#each temoignages}}
        <div class="container-avis">
            <h3 class="title-avis">{{this.type_seance}}</h3>
            <blockquote> <p>{{this.message}}</p>
            </blockquote>
            <div class="author-avis">{{this.signature}}</div>
        </div>
        {{/each}}

    </div>











</section>

<div class="link-articles"><a href="/temoignages">TOUS LES TEMOIGNAGES<img class="arrow-link"
